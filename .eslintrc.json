{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-underscore-dangle": ["error", { "allow": ["_id"]}],
        "linebreak-style": 0,
        "no-console": "off",
        "func-names": "off",
        "consistent-return": "off"
    }
}
